{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "definitions.json",
  "definitions": {
    "copyright": {
      "type": "string"
    },
    "date": {
      "type": "string",
      "pattern": "(\\d{4})-(\\d{2})-(\\d{2})"
    },
    "explanation": {
      "type": "string"
    },
    "hdurl": {
      "type": "string",
      "pattern": "^(?:http(s)?:\\/\\/)?[\\w.-]+(?:\\.[\\w\\.-]+)+[\\w\\-\\._~:/?#[\\]@!\\$&'\\(\\)\\*\\+,;=.]+$"
    },
    "media_type": {
      "type": "string"
    },
    "service_version": {
      "type": "string",
      "const": "v1"
    },
    "title": {
      "type": "string"
    },
    "url": {
      "type": "string",
      "pattern": "^(?:http(s)?:\\/\\/)?[\\w.-]+(?:\\.[\\w\\.-]+)+[\\w\\-\\._~:/?#[\\]@!\\$&'\\(\\)\\*\\+,;=.]+$"
    },
    "code": {
      "type": "number"
    },
    "msg": {
      "type": "string"
    }
  }
}
